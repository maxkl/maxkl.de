<div class="project-list">
    <div for(project in input.projects) class="project">
        <div class="thumbnail-container">
            <div style="width: 50px; height: 50px; background-color: lightgray;"></div>
        </div>
        <div class="body">
            <a href="/projects/${project.name}" class="title">${project.title}</a>
            <a if(project.link) href=project.link target="_blank" class="link">Open</a>
            ${project.shortDesc}
        </div>
    </div>
</div>
